<h1> <%= @list.name %></h1>
<% @list.bookmarks.each do |bookmark| %>
<div>
  <p><%= bookmark.movie.title %></p>
  <em><%= bookmark.comment %></em>
</div>

<% end %>
<br><br><br>
<p>add a bookmark</p>
<%= simple_form_for [@list, @bookmark] do |f| %>
<%= f.association :movie, collection: Movie.all  %>
<%= f.input :comment %>
<%= f.submit %>
<% end %>



